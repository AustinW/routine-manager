{
    "name": "austinw/routine-manager",
    "description": "Routine Manager allows coaches and athletes to structure their team and keep track of routines as well as performance.",
    "license": "proprietary",

    "repositories":
    [
        {
            "type": "vcs",
            "url": "https://github.com/AustinW/Laravel-MongoDB"
        }
    ],
    "require": {
        "laravel/framework": "4.1.*",
        "jenssegers/mongodb": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "way/phpunit-wrappers": "dev-master",
        "mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php",
            "app/libraries",
            "app/interfaces"
        ],
        "files": [
            "app/helpers.php"
        ],
        "folders": [
            "app/libraries"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
